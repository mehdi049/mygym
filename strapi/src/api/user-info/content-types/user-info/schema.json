{
  "kind": "collectionType",
  "collectionName": "user_infos",
  "info": {
    "singularName": "user-info",
    "pluralName": "user-infos",
    "displayName": "User info",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "first_name": {
      "type": "string"
    },
    "last_name": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "birthday": {
      "type": "date"
    },
    "profile_picture": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "account": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "Homme",
        "Femme"
      ]
    },
    "gyms": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::gym.gym",
      "mappedBy": "user_info"
    }
  }
}
